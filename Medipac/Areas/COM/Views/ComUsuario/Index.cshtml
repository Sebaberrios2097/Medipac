@model IEnumerable<Medipac.Areas.COM.Data.DTO.DtoComUsuario>

@{
    ViewData["Tittle"] = "Lista de Usuarios";
    Layout = "~/Views/Shared/_LayoutAdm.cshtml";
}

<h1 class="mt-3">@ViewData["Tittle"]</h1>
<hr />
<div class="card">
    <div class="card-header">
        <a class="btn btn-success">Crear Usuario</a>
        <a class="btn btn-primary">Volver</a>
    </div>
    <div class="card-body">
        <table id="myTable" class="table">
            <thead>
                <tr>
                    <th>Id Usuario</th>
                    <th>Nombre de Usuario</th>
                    <th>Fecha de Creación</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.IdUsuario</td>
                        <td>@item.Usuario</td>
                        <td>@item.FechaCreacion</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="card-footer">

    </div>
</div>

<script>
    var table = new DataTable('#myTable', {
        language: {
            url: '//cdn.datatables.net/plug-ins/2.1.8/i18n/es-CL.json',
        },
    });
</script>